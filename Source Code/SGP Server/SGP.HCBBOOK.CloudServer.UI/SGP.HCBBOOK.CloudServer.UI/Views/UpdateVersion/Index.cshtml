
@{
    ViewBag.Title = "Danh sách thay đổi version";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<SGP.HCBBOOK.CORE.Busssiness.Models.CheckVersionInfoModel>


<!-- Exportable Table -->
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    Danh sách Key Bản quyền
                </h2>
                <ul class="header-dropdown m-r--5">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            <i class="material-icons">more_vert</i>
                        </a>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="javascript:void(0);">Action</a></li>
                            <li><a href="javascript:void(0);">Another action</a></li>
                            <li><a href="javascript:void(0);">Something else here</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div class="body">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-hover dataTable js-exportable">
                        <thead>
                            <tr>
                                <th>STT</th>
                                <th>Version</th>
                                <th>Trạng thái</th>
                                <th>Mandatory</th>
                                <th>InstallerArgs</th>
                                <th>Checksum</th>
                                <th>HashingAlgorithm</th>
                                <th>SoftName</th>
                                <th>Người cập nhật</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>STT</th>
                                <th>Version</th>
                                <th>Trạng thái</th>
                                <th>Mandatory</th>
                                <th>InstallerArgs</th>
                                <th>Checksum</th>
                                <th>HashingAlgorithm</th>
                                <th>SoftName</th>
                                <th>Người cập nhật</th>
                            </tr>
                        </tfoot>
                        <tbody>
                            @{int stt = 0; }
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@(++stt)</td>
                                    <td><a href="@Url.Action("Detail", "UpdateVersion", new { id = item.ID })">@item.Version</a></td>
                                    @if (item.IsUpdateAvailable)
                                    {
                                        <td class="bg-green">Có sẵn</td>
                                    }
                                    else
                                    {
                                        <td class="bg-pink">Phiên bản củ</td>
                                    }
                                    <td>@item.Mandatory</td>
                                    <td>@item.InstallerArgs</td>
                                    <td>@item.Checksum</td>
                                    <td>@item.HashingAlgorithm</td>
                                    <td>@item.Softname</td>
                                    <td>@item.DateChange</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- #END# Exportable Table -->
